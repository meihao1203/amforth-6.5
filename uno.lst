000120 9a2c                          sbi OW_PORT, OW_BIT ; release bus
000121 9824                          cbi OW_DDR, OW_BIT
000122 e0e8
000123 e0f0
000124 9731
000125 f7f1                          delay 2
                                     ; re-enable interrupts
000126 bf1f                          out SREG, temp1
